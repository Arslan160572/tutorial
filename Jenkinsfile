pipeline {
    agent any
    stages {
      
      stage('Biuld') {
                steps {
                        echo 'It is building.'
			                }
                      }
	    
      
      stage('Test'){  
		steps { 
       echo 'It is testing.'
          }
	        }
    
    
    stage('Run') {
		steps {
          echo 'It is running.'
		      }
         }
        
        
        }
    }

